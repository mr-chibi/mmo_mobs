# Mob Hitpoints:
attribute @s minecraft:generic.max_health base set 75
data modify entity @s Health set value 75f

# Mob Attack Strength:
attribute @s minecraft:generic.attack_damage base set 11.5

# Mob Defense:
attribute @s minecraft:generic.armor base set 18
attribute @s minecraft:generic.armor_toughness base set 18

# Mob CustomName:
data modify entity @s CustomNameVisible set value 1b
data modify entity @s CustomName set value "{\"text\": \"Skeleton [Lvl 29]\", \"color\": \"white\"}"