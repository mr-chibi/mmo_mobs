# Mob Armors/Items:
data modify entity @s HandItems set value [{id:"minecraft:wooden_sword", Count:1b}, {}]
data modify entity @s ArmorItems set value [{id:"minecraft:leather_boots", Count:1b}, {id:"minecraft:leather_leggings", Count:1b}, {id:"minecraft:leather_chestplate", Count:1b}, {id:"minecraft:leather_helmet", Count:1b}]

# Armor Drop Chances:
data modify entity @s HandDropChances set value [0f, 0f]
data modify entity @s ArmorDropChances set value [0f, 0f, 0f, 0f]